{
    "name": "destech-hasar-cozumleri-a-s/larabite",
    "description": "Complete Flowbite component library for Laravel with Blade components, Tailwind CSS, Alpine.js, and Livewire support",
    "keywords": [
        "laravel",
        "flowbite",
        "blade",
        "components",
        "tailwind",
        "alpine",
        "livewire",
        "ui",
        "tailwindcss"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Destech Hasar Cozumleri Development Team",
            "email": "erman.titiz@icloud.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Destech\\Packages\\FlowbiteLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Destech\\Packages\\FlowbiteLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Destech\\Packages\\FlowbiteLaravel\\FlowbiteLaravelServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
